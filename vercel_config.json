{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "MYSQL_HOST": "@mysql_host",
    "MYSQL_PORT": "@mysql_port",
    "MYSQL_USER": "@mysql_user",
    "MYSQL_PASSWORD": "@mysql_password",
    "MYSQL_DATABASE": "@mysql_database"
  },
  "functions": {
    "main.py": {
      "runtime": "python3.9",
      "maxDuration": 300
    }
  }
}